{
  "name": "bpmppt-app",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Fery Wardiyanto",
    "email": "ferywardiyanto@gmail.com"
  },
  "ignore": [
    "**/.*",
    "node_modules",
    "application/vendor",
    "asset/bower"
  ],
  "dependencies": {
    "bootstrap": "~3.2.0",
    "bootstrap-datepicker": "~1.3.0",
    "bootstrap-switch": "~3.0.2",
    "datatables": "~1.10.2",
    "fineuploader-dist": "~5.0.7",
    "fontawesome": "~4.2.0",
    "jquery": "~2.1.1",
    "jquery-autosize": "~1.18.12",
    "jquery-mousewheel": "~3.1.12",
    "jquery-ui": "~1.11.1",
    "jqueryui-touch-punch": "*",
    "morrisjs": "~0.5.1",
    "select2": "~3.5.1",
    "summernote": "~0.5.9"
  },
  "devDependencies": {
    "codemirror": "~4.6.0",
    "datatables-plugins": "*"
  },
  "preen": {
    "bootstrap": [
      "dist/**",
      "LICENSE"
    ],
    "bootstrap-datepicker": [
      "css/*.css",
      "js/**/*.js",
      "LICENSE"
    ],
    "bootstrap-switch": [
      "dist/**",
      "LICENSE"
    ],
    "datatables": [
      "media/css/*.css",
      "media/images/*.png",
      "media/js/*.js",
      "license.txt"
    ],
    "datatables-plugins": [
      "integration/**",
      "i18n/*.lang"
    ],
    "fineuploader-dist": [
      "dist/placeholder/*",
      "dist/fineuploader.{js,min.js}",
      "dist/jquery.fineuploader.{js,min.js}",
      "dist/*.{css,gif}"
    ],
    "fontawesome": [
      "css/*.css",
      "fonts/*",
      "license.txt"
    ],
    "jquery": [
      "dist/*",
      "MIT-LICENSE.txt"
    ],
    "jquery-autosize": [
      "*.js"
    ],
    "jquery-mousewheel": [
      "*.js",
      "LICENSE.txt"
    ],
    "jquery-ui": [
      "themes/base/**/*",
      "ui/**/*.js",
      "LICENSE.txt"
    ],
    "jqueryui-touch-punch": [
      "*.js"
    ],
    "morrisjs": [
      "*.css",
      "*.js"
    ],
    "raphael": [
      "*.js",
      "license.txt"
    ],
    "select2": [
      "*.css",
      "*.js",
      "*.png",
      "*.gif",
      "LICENSE"
    ],
    "summernote": [
      "dist/*",
      "lang/*"
    ],
    "codemirror": [ "" ],
    "mocha": [ "" ]
  }
}
